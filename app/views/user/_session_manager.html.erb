<% if current_user %> 
    <div class="d-grid gap-2 d-md-flex justify-content-md-end">
        <%= button_to "Edit Account", edit_user_registration_path, class: "nav-link float-end btn btn-primary me-md-2 "%>
        <%= button_to "Log Out", destroy_user_session_path, class: "nav-link float-end btn btn-primary me-md-2 ", method: "delete" %> 
    </div>
<% else %>
    <div class="d-grid gap-2 d-md-flex justify-content-md-end">
        <%= link_to "Sign Up", new_user_registration_path, class: "nav-link float-end"%>
        <%= link_to "Log in", new_user_session_path, class: "nav-link float-end"%> 
    </div>
<% end %>